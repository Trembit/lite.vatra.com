{
  "name": "thevatra",
  "version": "0.3.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:dev": "ng build --configuration dev",
    "build:staging": "ng build --configuration staging",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "janus:build": "cd ./node_modules/janus-gateway/npm && npm install && npm run rollup -- --o ../../../src/assets/stusan/scripts/janus/janus.js --f es",
